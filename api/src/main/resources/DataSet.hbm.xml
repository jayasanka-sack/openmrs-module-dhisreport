<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.dhisreport.api.model">
    <class name="DataSet" table="${project.parent.artifactId}_dataset">
        <id column="dataset_id" name="id" unsaved-value="0">
            <generator class="native"/>
        </id>
        <discriminator column="dataset_id" insert="false"/>
        <property column="uuid" name="uuid"/>
        <property column="code" name="code"/>
        <property column="name" name="name"/>
        <property column="period_type" name="periodType"/>
        <property column="report_definition_uuid" name="reportDefinitionUuid"/>
        <set name="dataElements" table="${project.parent.artifactId}_dataset_data_element"
          lazy="true" fetch="select" cascade="all">
            <key>
                <column name="dataset_id" not-null="true"/>
            </key>
            <many-to-many class="DataElement">
                <column name="data_element_id" not-null="true"/>
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>
