<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.dhisreport.api.model">
    <class name="Category" table="${project.parent.artifactId}_datavalue_template">
        <id column="id" name="id" unsaved-value="0">
            <generator class="native"/>
        </id>
        <discriminator column="id" insert="false"/>
        <property column="name" name="name" length="230"/>
        <property column="uid" name="uid" length="11" unique="true"/>
        <property column="code" name="code" length="50"/>
    </class>
</hibernate-mapping>
